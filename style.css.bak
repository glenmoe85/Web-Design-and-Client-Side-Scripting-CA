body {
  margin: 0;
  font-family: Arial, Helvetica, sans-serif;
  background-color:#d7ecd9;
}
.container{
	border-left: 10px solid white;
	border-right: 10px solid white;
}

	
.banner{
	background-color:#f6f6eb;
	height:130px;
	margin: auto;
	text-align: center;
	 font-family: Brush Script MT;
	
}
.banner p{
	margin: 0px;
	 font-size: 25px;
	 
}
.banner h1{
	padding: 15px 0px 0px 0px;
	margin: 0px;
	 font-size: 35px;
	 
}
.topnav {
  overflow: hidden;
  background-color: #009b77;
}

.topnav a {
  float: left;
  display: block;
  color: #f6f6eb;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}

.topnav a:hover {
  background-color: #ddd;
  color: black;
}

.topnav a.active {
  background-color: #1e5631;
  color: white;
}

.topnav .icon {
  display: none;
}
/*Small screen - Tablets phone */
@media screen and (max-width: 640px) {
  .topnav a:not(:first-child) {display: none;}
  .topnav a{display:none;}
  .topnav a.icon {
    float: right;
    display: block;
  }
.topnav.responsive {position: relative;}
  .topnav.responsive .icon {
    position: absolute;
    right: 0;
    top: 0;
  }
.topnav.responsive a {
    float: none;
    display: block;
    text-align: left;
  }
}

.mainpgImg img {
   height: auto;
width: 100%;
display:block;

}
.intro{
	background-color:#f6f6eb;
	overflow: auto;
	padding: 10px 5% 40px 5%;
	
}
.intro h1{
	font-size: 25px;
	text-align: center;
}
.intro p {
	font-size: 20px;
	margin-bottom:0px;
	
}
.mainpgCounties{
	background-color:#009b77;
	overflow: auto;
	padding-bottom: 40px;
}

	
.imgrow {
	text-align:center;
}
.imgrow img {
  margin-top: 40px;
  width: 70%;
}
.imgleft{
	float: left;
  width: 45%;
  padding-left:5%;
}
.imgright{
	float: right;
  width: 45%;
  padding-right:5%;
}
.footer{
	background-color:#f6f6eb;
	padding: 10px 5%;
	font-size:13px;
	vertical-align: text-bottom;
	text-align: center;
	
}
/*Small screen - Tablets phone */
@media screen and (max-width: 640px) {


	.imgleft{
		width: 70%;
		float: none;
		padding-right: 15%;
		padding-left: 15%;
		
	}
	.imgright{
		width: 70%;
		float: none;
		padding-right: 15%;
		padding-left: 15%;
	}
}